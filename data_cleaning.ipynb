    "# 建立並訓練一個簡單的 PyTorch 神經網路\n",
    "\n",
    "# 1. 定義神經網路模型\n",
    "# 這是一個簡單的「多層感知器」(MLP)，由輸入層、兩個隱藏層和一個輸出層組成。\n",
    "class SimpleNN(nn.Module):\n",
    "    def __init__(self, input_size):\n",
    "        super(SimpleNN, self).__init__()\n",
    "        # 定義第一層：從 input_size 個輸入到 64 個神經元 (隱藏層)\n",
    "        self.layer1 = nn.Linear(input_size, 64)\n",
    "        self.relu = nn.ReLU() # ReLU 激活函數，引入非線性能力\n",
    "        \n",
    "        # 定義第二層：從 64 個神經元到 32 個神經元 (另一個隱藏層)\n",
    "        self.layer2 = nn.Linear(64, 32)\n",
    "        self.relu2 = nn.ReLU()\n",
    "        \n",
    "        # 定義輸出層：從 32 個神經元到 1 個輸出 (用於二元分類，預測生存率)\n",
    "        self.output_layer = nn.Linear(32, 1)\n",
    "        self.sigmoid = nn.Sigmoid() # Sigmoid 激活函數，將輸出壓縮到 0 到 1 之間 (表示機率)\n",
    "\n",
    "    # 定義前向傳播的路徑 (數據如何流過網路)\n",
    "    def forward(self, x):\n",
    "        x = self.layer1(x)\n",
    "        x = self.relu(x)\n",
    "        x = self.layer2(x)\n",
    "        x = self.relu2(x)\n",
    "        x = self.output_layer(x)\n",
    "        x = self.sigmoid(x)\n",
    "        return x\n",
    "\n",
    "# 2. 實例化模型、定義損失函數和優化器\n",
    "input_size = X_train_tensor.shape[1] # 輸入特徵的數量，由資料決定\n",
    "model = SimpleNN(input_size)\n",
    "\n",
    "criterion = nn.BCELoss() # 二元交叉熵損失 (Binary Cross-Entropy Loss)，適合二元分類問題\n",
    "optimizer = optim.Adam(model.parameters(), lr=0.001) # Adam 優化器，學習率 (lr) 決定每次參數更新的幅度\n",
